{
  "kind": "singleType",
  "collectionName": "page_membership",
  "info": {
    "name": "membership",
    "description": "membership page"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "meta.meta-data",
      "required": true
    },
    "banner": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "side_images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "membership_form_link": {
      "type": "string"
    },
    "settings": {
      "type": "component",
      "repeatable": false,
      "component": "settings.membership-settings",
      "required": true
    }
  }
}

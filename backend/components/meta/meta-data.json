{
  "collectionName": "component_metaData",
  "info": {
    "name": "MetaData",
    "icon": "globe-americas"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "required": true
    },
    "shareImage": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "twitterCardType": {
      "type": "enumeration",
      "enum": [
        "summary",
        "summary_large_image",
        "app",
        "player"
      ],
      "default": "summary"
    },
    "twitterUsername": {
      "type": "string"
    }
  }
}
